<div>
    <h2>#Blog nổi bật
    </h2>

    &nbsp;&nbsp;
    <div class="container text-center">
        <div class="row align-items-start">
            {{#each customBlog}}

            <a class="col" style="color: black; text-decoration: none; margin-left: 30px; margin-right:30px;"
                href="/blog/{{this.slug}}">
                <img src="{{this.image}}" alt="123" style="width : 150px;height:150px;  border-radius: 25px;">
                <p style="font-weight: bold;font-size:20px">{{this.title}}
                </p>
                <span class="text-secondary"></span>
            </a>
            {{/each}}
        </div>
    </div>
</div>

